{
  "name": "quhelp",
  "version": "1.0.0",
  "description": "趣帮帮 react版",
  "main": "index.js",
  "scripts": {
    "dev": "npm run start",
    "start": "NODE_ENV=development webpack-dev-server --mode development  --inline  --progress --colors",
    "build": "rm -rf dist/ && NODE_ENV=production webpack --config webpack.config.prod.js --mode production --progress --colors",
    "eslint": "eslint src/**/*.js",
    "fixeslint": "eslint src/**/*.js --fix",
    "stylelint": "stylelint src/**/*.scss",
    "pret": "prettier --single-quote --tab-width 2 --no-semi --trailing-comma all --write \"src/**/*.js\"",
    "precommit": "lint-staged",
    "commitmsg": "validate-commit-msg"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint"
    ],
    "src/**/*.scss": [
      "stylelint"
    ]
  },
  "author": "colorLei",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.1.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.5.0",
    "eslint-plugin-babel": "^5.2.0",
    "eslint-plugin-jsx": "^0.0.2",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "happypack": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "json-loader": "^0.5.7",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.9.3",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "stylelint": "^9.5.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "url-loader": "^1.0.1",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4",
    "yaml-loader": "^0.5.0"
  },
  "dependencies": {
    "antd": "^3.8.4",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "echarts": "^4.1.0",
    "immutable": "^3.8.2",
    "qs": "^6.5.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-saga": "^0.16.0"
  }
}
